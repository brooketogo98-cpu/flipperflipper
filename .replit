modules = ["bash", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["gmp", "iana-etc", "udev"]

[[ports]]
localPort = 4040
externalPort = 80

[[ports]]
localPort = 5000
externalPort = 80

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Stitch CLI"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Stitch Web Interface"

[[workflows.workflow]]
name = "Stitch CLI"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Stitch Web Interface"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 web_app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
