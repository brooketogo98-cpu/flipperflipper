{
  "name": "gaming-pc-arbitrage-extension",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "extension"
  ],
  "scripts": {
    "dev": "npm run dev -w extension",
    "build": "npm run build -w extension",
    "build:core": "npm run build -w @arbitrage/core",
    "build:all": "npm run build:core && npm run build",
    "test": "npm run test -w extension",
    "test:core": "npm run test -w @arbitrage/core",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules packages/*/node_modules extension/node_modules",
    "build-extension": "node build-extension.cjs"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "tesseract.js": "^5.0.4",
    "chart.js": "^4.4.1",
    "wavesurfer.js": "^7.6.2",
    "qrcode": "^1.5.3",
    "ical-generator": "^6.0.1",
    "simple-statistics": "^7.8.3",
    "ml-kmeans": "^6.0.0",
    "fs-extra": "^11.2.0",
    "archiver": "^6.0.1"
  }
}
